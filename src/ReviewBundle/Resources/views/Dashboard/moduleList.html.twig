<ul class="review-list">
    {% for module in modules %}
        <li class="module">
            <a href="{% block detailRoute %} {% endblock %}">
                <div class="font-weight-bold"> {% block headerLine %}{% endblock %} </div>
                <div> {% block subHeaderLine %} {% endblock %}  </div>
            </a>
        </li>
    {% else %}
        <li>Aucun retour sur ce module :( </li>
    {% endfor %}
</ul>